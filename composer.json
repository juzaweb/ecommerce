{
    "name": "juzaweb/ecommerce",
    "description": "Ecommerce plugin for website designed by Juzaweb",
    "extra": {
        "juzaweb": {
            "providers": [
                "Juzaweb\\Ecommerce\\Providers\\EcommerceServiceProvider"
            ],
            "name": "Ecommerces",
            "domain": "ecom",
            "cms_min": "3.0",
            "version": "1.0"
        }
    },
    "require": {
        "league/omnipay": "^3",
        "omnipay/paypal": "^3.0",
        "omnipay/stripe": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Juzaweb\\Ecommerce\\": "src/",
            "Juzaweb\\Ecommerce\\Tests\\": "tests/"
        },
        "files": [
            "vendor/autoload.php",
            "src/Helpers/helpers.php"
        ]
    },
    "replace": {
        "psr/http-message": "*",
        "guzzlehttp/guzzle": "*",
        "guzzlehttp/promises": "*",
        "guzzlehttp/psr7": "*",
        "league/flysystem": "*",
        "phpseclib/phpseclib": "*",
        "paragonie/random_compat": "*",
        "paragonie/constant_time_encoding": "*",
        "graham-campbell/guzzle-factory": "*"
    }
}
